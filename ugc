local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

a:DestroyGui()

local w = library:CreateWindow("Zoorox HUB") -- Creates the window

local b = w:CreateFolder("Standart") -- Creates the folder(U will put here your buttons,etc)

local a = w:CreateFolder("Extra") -- Creates the folder(U will put here your buttons,etc)

b:Button("King Rock",function()
    game:GetService("MarketplaceService"):PromptPurchase(game.Players.LocalPlayer, 13619497053)
end)

b:Button("Whosesoda",function()
    game:GetService("MarketplaceService"):PromptPurchase(game.Players.LocalPlayer, 13618795459)
end)

a:Box("ID Limited","number",function(value) -- "number" or "string"
    game:GetService("MarketplaceService"):PromptPurchase(game.Players.LocalPlayer, (value))
end)
